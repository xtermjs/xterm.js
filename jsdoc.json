{
  "source": {
    "include": [
      "src/xterm.js",
      "addons/attach/index.js",
      "addons/fit/index.js",
      "addons/linkify/index.js"
    ]
  },
  "opts": {
    "readme": "README.md",
    "template": "node_modules/docdash",
    "encoding": "utf8",
    "destination": "docs/",
    "recurse": true,
    "verbose": true
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}
